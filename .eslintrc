{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "jquery": true
  },
  "rules": {
    "indent": [2, 2],
    "no-console": 0,
    "no-alert": 0,
    "no-use-before-define": 0,
    "no-useless-escape": 0,
    "no-duplicate-imports": 0,
    "comma-dangle": 0,
    "react/jsx-no-bind": 0,
    "react/prop-types": 0,
    "react/require-render-return": 0,
  }
}
